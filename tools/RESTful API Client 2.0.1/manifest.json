{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "default.png",
      "default_title": "Poster"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "This is a Rest client app to test your API calls...",
   "icons": {
      "128": "Poster.png",
      "16": "Poster.png",
      "48": "Poster.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "RESTful API Client",
   "permissions": [ "tabs", "activeTab", "geolocation", "http://*/", "https://*/" ],
   "short_name": "Rest Client",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.1"
}
